{
  "basePdf": {
    "width": 210,
    "height": 297,
    "padding": [10, 10, 10, 10]
  },
  "dpartOptions": {
    "enabled": true,
    "version": "PDF/VT-1",
    "mapping": {
      "ContactName": "name",
      "RecordID": "id",
      "MemberCode": "barcode",
      "PageCount": "pageCount"
    }
  },
  "schemas": [
    [
      {
        "name": "name",
        "type": "text",
        "position": { "x": 20, "y": 20 },
        "width": 100,
        "height": 10
      },
      {
        "name": "id",
        "type": "text",
        "position": { "x": 20, "y": 35 },
        "width": 100,
        "height": 10
      },
      {
        "name": "barcode",
        "type": "qrcode",
        "position": { "x": 150, "y": 20 },
        "width": 30,
        "height": 30
      },
      {
        "name": "photo",
        "type": "image",
        "position": { "x": 20, "y": 60 },
        "width": 50,
        "height": 50
      }
    ],
    [
      {
        "name": "email",
        "type": "text",
        "position": { "x": 20, "y": 20 },
        "width": 150,
        "height": 10
      },
      {
        "name": "phone",
        "type": "text",
        "position": { "x": 20, "y": 35 },
        "width": 150,
        "height": 10
      },
      {
        "name": "address",
        "type": "text",
        "position": { "x": 20, "y": 50 },
        "width": 200,
        "height": 20
      },
      {
        "name": "membershipLevel",
        "type": "text",
        "position": { "x": 20, "y": 75 },
        "width": 100,
        "height": 10
      }
    ]
  ]
}